<?xml version="1.0" encoding="UTF-8"?>
<project name="module_social_sdk_library_project" default="compile.module.social_sdk_library_project">
  <dirname property="module.social_sdk_library_project.basedir" file="${ant.file.module_social_sdk_library_project}"/>
  
  <property name="module.jdk.home.social_sdk_library_project" value="${jdk.home.android_api_20_platform}"/>
  <property name="module.jdk.bin.social_sdk_library_project" value="${jdk.bin.android_api_20_platform}"/>
  <property name="module.jdk.classpath.social_sdk_library_project" value="jdk.classpath.android_api_20_platform"/>
  
  <property name="compiler.args.social_sdk_library_project" value="-encoding UTF-8 -source 1.6 ${compiler.args}"/>
  
  <property name="social_sdk_library_project.output.dir" value="${module.social_sdk_library_project.basedir}/build/intermediates/classes/debug"/>
  <property name="social_sdk_library_project.testoutput.dir" value="${module.social_sdk_library_project.basedir}/build/intermediates/classes/androidTest/debug"/>
  
  <path id="social_sdk_library_project.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="social_sdk_library_project.module.production.classpath">
    <path refid="${module.jdk.classpath.social_sdk_library_project}"/>
  </path>
  
  <path id="social_sdk_library_project.runtime.production.module.classpath">
    <pathelement location="${social_sdk_library_project.output.dir}"/>
  </path>
  
  <path id="social_sdk_library_project.module.classpath">
    <path refid="${module.jdk.classpath.social_sdk_library_project}"/>
    <pathelement location="${social_sdk_library_project.output.dir}"/>
  </path>
  
  <path id="social_sdk_library_project.runtime.module.classpath">
    <pathelement location="${social_sdk_library_project.testoutput.dir}"/>
    <pathelement location="${social_sdk_library_project.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.social_sdk_library_project">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.social_sdk_library_project">
    <patternset refid="excluded.from.module.social_sdk_library_project"/>
  </patternset>
  
  <path id="social_sdk_library_project.module.sourcepath">
    <dirset dir="${module.social_sdk_library_project.basedir}">
      <include name="build/generated/source/r/debug"/>
      <include name="build/generated/source/aidl/debug"/>
      <include name="build/generated/source/buildConfig/debug"/>
      <include name="build/generated/source/rs/debug"/>
      <include name="build/generated/res/rs/debug"/>
      <include name="res"/>
      <include name="res"/>
    </dirset>
  </path>
  
  <path id="social_sdk_library_project.module.test.sourcepath">
    <dirset dir="${module.social_sdk_library_project.basedir}">
      <include name="build/generated/source/r/androidTest/debug"/>
      <include name="build/generated/source/aidl/androidTest/debug"/>
      <include name="build/generated/source/buildConfig/androidTest/debug"/>
      <include name="build/generated/source/rs/androidTest/debug"/>
      <include name="build/generated/res/rs/androidTest/debug"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.social_sdk_library_project" depends="compile.module.social_sdk_library_project.production,compile.module.social_sdk_library_project.tests" description="Compile module social_sdk_library_project"/>
  
  <target name="compile.module.social_sdk_library_project.production" depends="register.custom.compilers" description="Compile module social_sdk_library_project; production classes">
    <mkdir dir="${social_sdk_library_project.output.dir}"/>
    <javac2 destdir="${social_sdk_library_project.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.social_sdk_library_project}/javac">
      <compilerarg line="${compiler.args.social_sdk_library_project}"/>
      <bootclasspath refid="social_sdk_library_project.module.bootclasspath"/>
      <classpath refid="social_sdk_library_project.module.production.classpath"/>
      <src refid="social_sdk_library_project.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.social_sdk_library_project"/>
    </javac2>
    
    <copy todir="${social_sdk_library_project.output.dir}">
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/source/r/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/source/aidl/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/source/buildConfig/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/source/rs/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/res/rs/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/res">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/res">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.social_sdk_library_project.tests" depends="register.custom.compilers,compile.module.social_sdk_library_project.production" description="compile module social_sdk_library_project; test classes" unless="skip.tests">
    <mkdir dir="${social_sdk_library_project.testoutput.dir}"/>
    <javac2 destdir="${social_sdk_library_project.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.social_sdk_library_project}/javac">
      <compilerarg line="${compiler.args.social_sdk_library_project}"/>
      <bootclasspath refid="social_sdk_library_project.module.bootclasspath"/>
      <classpath refid="social_sdk_library_project.module.classpath"/>
      <src refid="social_sdk_library_project.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.social_sdk_library_project"/>
    </javac2>
    
    <copy todir="${social_sdk_library_project.testoutput.dir}">
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/source/r/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/source/aidl/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/source/buildConfig/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/source/rs/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.social_sdk_library_project.basedir}/build/generated/res/rs/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.social_sdk_library_project" description="cleanup module">
    <delete dir="${social_sdk_library_project.output.dir}"/>
    <delete dir="${social_sdk_library_project.testoutput.dir}"/>
  </target>
</project>